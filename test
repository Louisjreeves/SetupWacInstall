from colorama import Fore, Back, Style

# For Font color Use Fore.<color>
print(Fore.GREEN + "Using Fore.GREEN Prints Text with Color Green")
print(Fore.YELLOW + "Using Fore.YELLOQ Prints Text with yellow color")
print(Fore.RED + "Using Fore.RED Prints Text in Red Color")
print(Fore.LIGHTCYAN_EX + "Using Fore.CYAN Prints Text in Cyan color")
print(Fore.LIGHTBLUE_EX + "Using Fore.LIGHTBLUE_EX Prints Text in light blue color")
